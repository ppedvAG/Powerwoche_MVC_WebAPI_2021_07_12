{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LiveReload": {
    "LiveReloadEnabled": true,
    "ClientFileExtensions": ".cshtml,.css,.js,.htm,.html,.ts,.wc",
    "ServerRefreshTimeout": 3000,
    "WebSocketUrl": "/__livereload",
    "FolderToMonitor": "~/" 
  }
}
